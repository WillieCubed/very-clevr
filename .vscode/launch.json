  "version": "0.2.0",
  "configurations": [
    {
      "name": "Demo Model",
      "type": "python",
      "request": "launch",
      "module": "very_clevr",
      "args": ["interact"],
      "justMyCode": true
    },
    {
      "name": "Quickstart",
      "type": "python",
      "request": "launch",
      "module": "app"
    },
    {
      "name": "Train Model",
      "type": "python",
      "request": "launch",
      "module": "very_clevr",
      "args": ["train"],
      "justMyCode": true
    },
    {
      "name": "Start Chrome",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:7860",
      "webRoot": "${workspaceFolder}"
    }
  ],
  "compounds": [
    {
      "name": "Launch",
      "configurations": ["Quickstart", "Start Chrome"]
    }
  ]
}
